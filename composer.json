{
  "name": "generoi/wp-genero-woocommerce-blocks",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "WooCommerce blocks for the gutenberg editor",
  "homepage": "https://github.com/generoi/wp-genero-woocommerce-blocks",
  "authors": [
    {
      "name": "Oskar Schöldström",
      "email": "public@oxy.fi"
    }
  ],
  "keywords": [
    "wordpress",
    "gutenberg"
  ],
  "support": {
    "issues": "https://github.com/generoi/wp-genero-woocommerce-blocks/issues"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.minasithil.genero.fi/"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.4.2",
    "wp-cli/wp-cli-bundle": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "GeneroWP\\WooCommerceBlocks\\": "src/"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpcs"
    ]
  },
  "archive" : {
    "exclude": [
      ".gitignore"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
